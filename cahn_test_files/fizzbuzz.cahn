let fizz_count := 1000

let i := 1
while i <= fizz_count {
    let output := ""

    if i % 3 == 0 {
        output := output .. "Fizz"
    }

    if i % 5 == 0 {
        output := output .. "Buzz"
    }

    if output == "" {
        print i
    } else {
        print output
    }

    i := i + 1
}